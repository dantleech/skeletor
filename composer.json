{
    "name": "dantleech/skeletor",
    "description": "Project Skeleton Generator",
    "type": "lib",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/console": "^4.0",
        "symfony/filesystem": "^4.0",
        "symfony/process": "^4.0",
        "symfony/debug": "^4.0",
        "dnoegel/php-xdg-base-dir": "^0.1.0",
        "seld/jsonlint": "^1.4",
        "justinrainbow/json-schema": "^1.6",
        "pimple/pimple": "^3.0",
        "padraic/phar-updater": "^1.0",
        "webmozart/path-util": "^2.3",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "Skeletor\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skeletor\\Tests\\": "tests/"
        }
    }
}
